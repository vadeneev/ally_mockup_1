{
  "name": "ally_mockup_1",
  "version": "1.0.0",
  "description": "Ally mockup implementation",
  "main": "index.js",
  "scripts": {
    "build:styles": "sass src/styles/index.scss docs/styles/index.css",
    "build:images": "cp -R src/images docs/images",
    "build:scripts": "webpack -c webpack.config.js",
    "build:html": "cp src/index.html docs/index.html",
    "clean:build": "rm -rf docs",
    "build": "npm run clean:build && mkdir docs && npm run build:html && npm run build:styles && npm run build:images && npm run build:scripts",
    "serve": "http-server -o docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vadeneev/ally_mockup_1.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vadeneev/ally_mockup_1/issues"
  },
  "homepage": "https://github.com/vadeneev/ally_mockup_1#readme",
  "dependencies": {
    "http-server": "^0.12.3",
    "nodemon": "^2.0.7",
    "path": "^0.12.7",
    "rollup": "^2.39.0",
    "sass": "1.32.7",
    "webpack": "^5.23.0",
    "webpack-cli": "^4.5.0",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@babel/core": "^7.12.17",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/preset-env": "^7.12.17",
    "babel-loader": "^8.2.2"
  }
}
