{
  "name": "ally_mockup_1",
  "version": "1.0.0",
  "description": "Ally mockup implementation",
  "main": "index.js",
  "scripts": {
    "build:styles": "sass src/styles/index.scss dist/styles/index.css",
    "build:images": "cp -R src/images dist/images",
    "build:scripts": "cp -R src/scripts dist/scripts",
    "build:html": "cp src/index.html dist/index.html",
    "clean:build": "rm -rf dist",
    "build": "npm run clean:build && mkdir dist && npm run build:html && npm run build:styles && npm run build:images && npm run build:scripts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vadeneev/ally_mockup_1.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vadeneev/ally_mockup_1/issues"
  },
  "homepage": "https://github.com/vadeneev/ally_mockup_1#readme",
  "dependencies": {
    "sass": "1.32.7"
  },
  "devDependencies": {
  }
}
